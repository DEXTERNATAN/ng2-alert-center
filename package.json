{
  "name": "ng2-alert-center",
  "version": "0.4.2",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve -aot -prod",
    "test": "npm run clean && ng lint && ng test --singleRun=true --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm run clean && npm run build.demo && npm run build.modules && npm run clean.modules.generates",
    "build.modules": "npm run build.modules.transpile && npm run build.modules.bundle",
    "build.modules.transpile": "ngc -p ./src/tsconfig.modules.json",
    "build.modules.bundle": "rollup -c && uglifyjs index.umd.js --screw-ie8 --compress --mangle --comments --output index.umd.min.js",
    "build.demo": "ng build -aot -prod",
    "clean": "npm run clean.demo && npm run clean.modules && npm run clean.npm-package",
    "clean.demo": "rm -rf demo",
    "clean.modules": "npm run clean.modules.compilates && npm run clean.modules.generates",
    "clean.modules.compilates": "rm -rf alert-center && rm -f index.*",
    "clean.modules.generates": "find ./src -name '*.ngfactory.ts' -delete && find ./src -name '*.ngsummary.json' -delete && rm -rf ./src/node_modules",
    "clean.npm-package": "rm -rf npm-package",
    "link": "mkdir npm-package && cp package.json npm-package && cp -r alert-center npm-package && cp index.* npm-package && cd npm-package && npm link && cd .."
  },
  "keywords": [
    "ng",
    "ng2",
    "angular",
    "@angular",
    "angular2",
    "twitter-bootstrap",
    "bootstrap",
    "bootstrap3",
    "alert",
    "center",
    "alert-center",
    "notification"
  ],
  "main": "index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "repository": "https://github.com/zsfarkas/ng2-alert-center.git",
  "private": false,
  "dependencies": {},
  "peerDependencies": {
    "@angular/common": "*",
    "@angular/compiler": "*",
    "@angular/core": "*",
    "@angular/animations": "*",
    "bootstrap": "^3.3.7",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/cli": "1.3.2",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "bootstrap": "^3.3.7",
    "codelyzer": "~3.1.1",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "protractor": "~5.1.2",
    "rollup": "^0.48.2",
    "rxjs": "^5.4.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "uglify-js": "^3.0.28",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.14"
  }
}
